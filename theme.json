		"useRootPaddingAwareAlignments": false
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--dark-green)",
			"text": "var(--wp--preset--color--base)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--bs-border-color)",
					"radius": "var(--bs-border-radius)",
					"style": false,
					"width": "var(--bs-border-width)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"top": "0.375rem",
						"right": "0.75rem",
						"bottom": "0.375rem",
						"left": "0.75rem"
					}
				}
			},
			"caption": {
				"color": {
					"text": "#ffffffbf"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "16px"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	],
	"version": 2
}
