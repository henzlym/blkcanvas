{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bca/bootstrap-container",
	"version": "0.1.0",
	"title": "Bootstrap Container",
	"category": "design",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"backgroundColor": {
			"type": "string",
			"default": "transparent"
		},
		"textColor": {
			"type": "string",
			"default": "inherit"
		},
		"tagName": {
			"type": "string",
			"default": "div"
		},
		"fullWidth": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "bootstrap-container",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
